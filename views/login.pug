extends layout

block content
  h1 Login
  .container
    form(method='POST', action='/users/login')
      #form-group
        label Username:
        input.form-control(name='name', type='text')
      #form-group
        label Password:
        input.form-control(name='email', type='text')
        br
        input.btn.btn-primary(type='submit', value='Submit')
